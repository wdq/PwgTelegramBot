	<log4net>
		<appender name="RollingFile" type="log4net.Appender.RollingFileAppender">
			<file value="C:\Users\wquade\Desktop\logs\PwgTelegramBot\log4net.log" />
			<appendToFile value="true" />
			<maximumFileSize value="500KB" />
			<maxSizeRollBackups value="2" />
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%date %level %logger - %message%newline" />
			</layout>
		</appender>
		<appender name="GelfUdpAppender" type="gelf4net.Appender.GelfUdpAppender, Gelf4net">
			<remoteAddress value="h.quade.co" />
			<remotePort value="12302" />
			<layout type="Gelf4net.Layout.GelfLayout, Gelf4net">
				<param name="AdditionalFields" value="app:PwgTelegramBot,version:1.0,Level:%level" />
				<param name="Facility" value="Phoenix Web Group" />
				<param name="IncludeLocationInformation" value="true" />
			</layout>
		</appender>
		<root>
			<level value="All" />
			<appender-ref ref="RollingFile" />
			<appender-ref ref="GelfUdpAppender" />
		</root>
	</log4net>	
